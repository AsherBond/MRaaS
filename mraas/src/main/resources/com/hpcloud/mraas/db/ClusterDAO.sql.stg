group ClusterDAO;

createClustersTable() ::= <<
  create table clusters (id Serial primary key, numNodes int)
>>

findById() ::= <<
  select id, numNodes from clusters where id= :id
>>


create() ::= <<
  insert into clusters(id, numNodes) values (:id, :numNodes)
>>

findAll() ::= <<
  select id, numNodes from clusters
>>
