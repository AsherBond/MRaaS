#!/usr/bin/ruby
# start the h2 database shell, and pop up a sql ui in your browser
dir = File.dirname(__FILE__)
jar = "~/.m2/repository/com/h2database/h2/1.3.158/h2*.jar"
db = "#{dir}/../target/mraas"
puts "\nStarting h2 DB shell.  <CTRL-C> to quit.\n\n"
exec "java -jar #{jar} -web -url jdbc:h2:#{db} -user mraas -password mraas"
